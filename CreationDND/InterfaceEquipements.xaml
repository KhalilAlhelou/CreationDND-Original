<Window x:Class="CreationDND.InterfaceEquipements"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CreationDND"
        mc:Ignorable="d"
        Title="InterfaceEquipements" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="Equipement1" IsEnabled="False" Margin="268,16,268,376"/>

        <ComboBox x:Name="Equipement2" IsEnabled="False" Margin="268,107,268,281"/>

        <ComboBox x:Name="Equipement3" IsEnabled="False" Margin="268,192,268,199"/>

        <ComboBox x:Name="Equipement4" IsEnabled="False" Margin="268,283,268,102"/>

        <TextBlock x:Name="EquipementPreSet" Margin="268,363,268,19"/>

        <Button x:Name="btnRetour" Content="Retour" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="38" Width="126"
                Click="btnRetourDePageEquipements_Click" FontFamily="Andalus" FontSize="20"/>

        <Button x:Name="ChoisirEquipement" Content="Choisir Equipement" Margin="596,354,19,28" FontFamily="Andalus" FontSize="18" Click="onClickBtn" ClickMode="Press"/>
    </Grid>
</Window>
